<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" android:state_pressed="false">
        <shape>
            <stroke android:color="@color/divider_color" android:width="0.1dp"/>
            <solid android:color="@color/white"/>
        </shape>
    </item>
    <item android:state_enabled="true" android:state_pressed="true">
        <shape>
            <stroke android:width="0.1dp" android:color="@color/divider_color" />
            <solid android:color="@color/press_color" />

        </shape>
    </item>
</selector>